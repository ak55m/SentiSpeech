{
    "version": 2,
    "functions": {
        "api/analyze.py": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "routes": [
        {
            "src": "/api/analyze",
            "dest": "/api/analyze.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "env": {
        "PYTHONUNBUFFERED": "1",
        "FLASK_ENV": "production"
    }
} 